{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "abraham/twitteroauth": "^3.3",
        "erusev/parsedown": "^1.7",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0",
        "laravel/nova": "^3.31",
        "laravel/tinker": "^2.7",
        "predis/predis": "^1.1",
        "spatie/laravel-mix-preload": "^1.0",
        "spatie/laravel-responsecache": "^7.2",
        "spatie/laravel-sluggable": "^3.3",
        "spatie/laravel-tail": "^4.4",
        "spatie/laravel-view-models": "^1.5",
        "spatie/schema-org": "^3.9",
        "timothyasp/nova-color-field": "^1.0",
        "spatie/laravel-ignition": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "doctrine/dbal": "^3.3",
        "laravel/envoy": "^2.8",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "fakerphp/faker": "^1.9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish --ansi",
            "@php artisan vendor:publish --tag=nova-views --force --ansi",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
